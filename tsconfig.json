{
  "compilerOptions": {
    "target": "es5",
    "lib": ["esnext","DOM.Iterable"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    // "strict": true,
    "jsx": "react"
  },
  "include": [
    ""
  ]
}
